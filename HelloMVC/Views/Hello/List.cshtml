@model List<Car>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>List</h2>
@foreach (var item in Model)
{
    <div>
        <ul>

            <li>
                @item.Brand ------- @item.LicensePlate
            </li>
        
        </ul>

    </div>
}


<div>@ViewBag.DisplayMessage</div>


